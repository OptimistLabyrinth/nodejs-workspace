{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": false,
    },
    "transform": {},
    "target": "es2020",
    "loose": false,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": true,
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false,
  },
  "minify": false,
  "isModule": true,
  "sourceMaps": true,
  "exclude": [
    "docs",
    "node_modules",
    "tools",
    "jest.config.ts"
  ],
}